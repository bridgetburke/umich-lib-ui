{
  "name": "umich-lib-components-react",
  "version": "0.0.0-development",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-info": "^3.4.5",
    "@storybook/addon-knobs": "^3.4.6",
    "@storybook/addon-storyshots": "^3.4.6",
    "@storybook/react": "^3.4.5",
    "@storybook/storybook-deployer": "^2.3.0",
    "cz-conventional-changelog": "^2.1.0",
    "prop-types": "^15.6.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.4.0",
    "semantic-release": "^15.6.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlibrary/umich-lib-components-react.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "release": {
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "branch": "master"
  }
}
