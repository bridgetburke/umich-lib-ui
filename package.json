{
  "name": "umich-lib-components-react-build-template",
  "version": "0.0.0-development",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-live": "^1.11.0"
  },
  "author": {
    "name": "Jon Earley",
    "email": "earleyj@umich.edu",
    "url": "https://github.com/jonearley"
  },
  "keywords": [
    "components",
    "design system",
    "react",
    "umich library",
    "mlibrary"
  ],
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.6",
    "@storybook/addon-storyshots": "^3.4.6",
    "@storybook/react": "^3.4.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "commitizen": "^2.10.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "markdown-to-jsx": "^6.7.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.0",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build:docs": "node scripts/build.js",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "test": "node scripts/test.js --env=jsdom",
    "build:css": "node-sass-chokidar src/ -o src/",
    "watch:sass": "npm run build:css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start:storybook": "npm-run-all --parallel start:storybook watch:sass",
    "deploy:storybook": "storybook-to-ghpages",
    "commit": "git-cz",
    "copy:css": "cpx \"./src/components/**/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:commonjs copy:css build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "deploy:docs": "npm run deploy:storybook",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "prepublish": "npm run build:lib",
    "gen:component-data": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:component-data -- --watch",
    "start:docs": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlibrary/umich-lib-components-react.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "lib"
  ],
  "homepage": "https://mlibrary.github.io/umich-lib-components-react/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": "commonjs"
        }
      ],
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
